""""""
# collatz problemi
n=int(input("n: "))
a=0
b=0
print("{}".format(n),end='')
while(a<1):
    if(n==1):
        print("\n{} iterasyon sürmüştür.".format(b))
        a=1
    else:
        if(n%2==0):
            n=int(n/2)
            b += 1
            print("->{}".format(n),end='')
        else:
            n=(3*n)+1
            b += 1
            print("->{}".format(n), end='')
""""""
