def fact(a):
    b=1
    for i in range(1,a+1):
       b*=i
    return b
x=int(input("x: "))
y=int(input("y: "))
k=(fact(x))/(fact(y)*fact(x-y))
print(int(k))
""""""
def fact(a):
    b=1
    for i in range(1,a+1):
       b*=i
    return b
x=int(input("x: "))
y=int(input("y: "))
k=(fact(x))/(fact(x-y))
print(int(k))
